{
	"extends": "eslint:recommended",
	"parser": "@babel/eslint-parser",
	"requireConfigFile": false,
	"plugins": ["@babel"],
	"env": {
		"node": true,
		"es2021": true,
		"jest": true
	},
	"rules": {
		"indent": ["error", "tab"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"no-const-assign": "error",
		"no-unused-vars": "error",
		"no-duplicate-imports": ["error", { "includeExports": true }]
	}
}